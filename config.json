{
  "log": {
    "loglevel": "warning",
    "access": "/dev/null",
    "error": "/dev/null"
  },
  "inbounds": [{
    "port": 3000,
    "protocol": "vmess",
    "tag": "mtn-proxy",
    "settings": {
      "clients": [{
        "id": "b6db55ef-ae53-425c-8fab-144c75df7cc2,
        "alterId": 0,
        "level": 0
      }],
      "disableInsecureEncryption": true
    },
    "streamSettings": {
      "network": "ws",
      "security": "none",
      "wsSettings": {
        "path": "/mtn-ug",
        "headers": {
          "Host": "mtn-uganda-proxy.kuberns.app"
        }
      }
    }
  }],
  "outbounds": [{
    "protocol": "freedom",
    "settings": {},
    "tag": "direct"
  }],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [{
      "type": "field",
      "ip": [
        "geoip:private"
      ],
      "outboundTag": "direct"
    }]
  },
  "policy": {
    "levels": {
      "0": {
        "handshake": 2,
        "connIdle": 120
      }
    }
  }
}
